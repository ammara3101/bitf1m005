@model List<Ead_project.Models.Theme>

@{
    ViewBag.Title = "show_events";
}



<!DOCTYPE html>

<html>
<head>
    <title>Free Brighton Website Template | Services :: w3layouts</title>
    <link href="~/Content/style.css" rel="stylesheet" type="text/css" media="all" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
</head>
<body id="admin">
    <script src="/Scripts/jquery.min.js"></script>
    <script>
        $(document).ready(function () {
             $("#ajax_btn").click(function () {
                $.ajax('@Url.Action("show_theme")', {

                    success: function (result) {
                        $("#ajax_show").html(result)
                    },

                });
            });


        });

    </script>
    <br><br><br><br>
    <p class="f1" style="color:deeppink">
        All Users details!!
    </p><br><br> <br><br>
    <div class="table_div">
        <table class="show_table">
            <tr>
                <th>Event Theme</th>
            </tr>
            @foreach (var item in Model)
            {

                <tr>
                    <td>@item.theme_name</td>
                </tr>

            }

        </table>
    </div>
    <br><br><br><br><br><br><br><br>
    <div class="table_div"></div>
        <div class="clear"></div>
</body>
</html>
