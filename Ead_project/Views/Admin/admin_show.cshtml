
@model List<Ead_project.Models.Theme>
@{
    ViewBag.Title = "admin_show";
}
<!DOCTYPE html>
<html>
<head>
    <title>Free Brighton Website Template | Services :: w3layouts</title>
    <link href="~/Content/style.css" rel="stylesheet" type="text/css" media="all" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
</head>
<body id="admin">
    <br><br><br><br><br><br>
    <p class="f1" style="color:deeppink">
        Show ALL Events details!!
    </p>
    <div id="add_image" class="image_bar" style="background-image:url('admin_jpg.jpg')"></div>
    <div class="table_div">
        <table class="show_table">
            <tr>
                <th>Event Theme</th>
                <th>Event Arrangement</th>
                <th>Seating Arrangement</th>
                <th>Stage Arrangement</th>
                <th>Stage  Seating</th>
                <th>Stage Lightening</th>
                <th>Budget</th>
            </tr>
            @foreach (var item in Model)
            {

                <tr>
                    <td>@item.theme_name</td>
                    <td>@item.color_combination</td>
                    <td>@item.seating_arrangnment</td>
                    <td>@item.stage_arrangnment</td>
                    <td>@item.stage_seating</td>
                    <td>@item.stage_lightening</td>
                    <td>@item.budget</td>
                </tr>

            }

        </table>
    </div>
    <br><br><br><br><br><br><br><br>
    <div class="clear"></div>
</body>
</html>